{
  "build": {
    "env": {
      "VERCEL_DEBUG": "1",
      "VERCEL_EDGE_FUNCTIONS_BRIDGE_PKG": "https://edge-functions-bridge-01-nextjs-middleware-dgee0beo8.vercel.app/edge.tgz",
      "VERCEL_NODE_BRIDGE_PKG": "https://edge-functions-bridge-01-nextjs-middleware-dgee0beo8.vercel.app/node.tgz"
    }
  },
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://test.com",
      "permanent": false,
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*bytespider.*"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/vercel-files/:path*",
      "destination": "https://pick-n-shovel.s3.amazonaws.com/vercel-files/:path*"
    }
  ],
  "headers": [
    {
      "source": "/vercel-files/(.*)",
      "headers": [
        {
          "key": "cdn-cache-control",
          "value": "public, max-age=86400, s-maxage=86400, stale-while-revalidate=86400"
        }
      ]
    }
  ]
}