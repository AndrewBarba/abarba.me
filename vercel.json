{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://test.com",
      "permanent": false,
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*bytespider.*"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/vercel-files/:path*",
      "destination": "https://pick-n-shovel.s3.amazonaws.com/vercel-files/:path*"
    }
  ],
  "headers": [
    {
      "source": "/vercel-files/(.*)",
      "headers": [
        {
          "key": "cdn-cache-control",
          "value": "max-age=15, stale-while-revalidate=120"
        }
      ]
    }
  ]
}